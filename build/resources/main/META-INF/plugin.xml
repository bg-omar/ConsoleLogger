<idea-plugin>
  <version>0.0.4</version>
  <change-notes>&lt;h2&gt;[0.0.4]&#xD;&lt;/h2&gt;
&lt;ul&gt;&lt;li&gt;shortcut for LOG insertion   (CTRL + ALT + 1)&#xD;&lt;/li&gt;&lt;li&gt;shortcut for INFO insertion  (CTRL + ALT + 2)&#xD;&lt;/li&gt;&lt;li&gt;shortcut for DEBUG insertion (CTRL + ALT + 3)&#xD;&lt;/li&gt;&lt;li&gt;shortcut for WARN insertion  (CTRL + ALT + 4)&#xD;&lt;/li&gt;&lt;li&gt;shortcut for ERROR insertion (CTRL + ALT + 5)&#xD;&lt;/li&gt;&lt;li&gt;shortcut for TABLE insertion (CTRL + ALT + 6)&#xD;&lt;/li&gt;&lt;li&gt;shortcut for deletion of all (CTRL + ALT + 0)&#xD;
-&#xD;
&#xD;
[0.0.4]: &lt;a href="https://github.com/bg-omar/consolelogger/compare/v0.0.3...v0.0.4"&gt;https://github.com/bg-omar/consolelogger/compare/v0.0.3...v0.0.4&lt;/a&gt;&#xD;&lt;/li&gt;&lt;/ul&gt;</change-notes>
  <description>&lt;p&gt;ConsoleLogger&lt;br /&gt;
An Intellij plugin to quickly insert console. ...()&lt;br /&gt;
Features:&lt;/p&gt;
&lt;ul&gt;&lt;li&gt;Insert logs&lt;/li&gt;&lt;li&gt;shortcut for LOG insertion   (CTRL + ALT + 1)&lt;/li&gt;&lt;li&gt;shortcut for INFO insertion  (CTRL + ALT + 2)&lt;/li&gt;&lt;li&gt;shortcut for DEBUG insertion (CTRL + ALT + 3)&lt;/li&gt;&lt;li&gt;shortcut for WARN insertion  (CTRL + ALT + 4)&lt;/li&gt;&lt;li&gt;shortcut for ERROR insertion (CTRL + ALT + 5)&lt;/li&gt;&lt;li&gt;shortcut for TABLE insertion (CTRL + ALT + 6)&lt;/li&gt;&lt;li&gt;shortcut for deletion of all (CTRL + ALT + 0) &lt;/li&gt;&lt;/ul&gt;</description>
  <id>com.github.bgomar.consolelogger</id>
  <name>ConsoleLogger</name>
  <vendor email="oscars.webspam@gmail.com" url="https://github.com/bg-omar/consolelogger">bgOmar</vendor>
  <idea-version since-build="222" until-build="223.*" />
  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.lang</depends>
  <depends>JavaScript</depends>
  <extensions defaultExtensionNs="com.intellij">
    <applicationConfigurable parentId="tools" instance="com.github.bgomar.consolelogger.settings.ConsoleLoggerSettingsConfigurable" id="com.github.bgomar.consolelogger.settings.ConsoleLoggerSettingsConfigurable" displayName="ConsoleLogger" />
    <applicationService serviceImplementation="com.github.bgomar.consolelogger.settings.ConsoleLoggerSettings" />
    <postStartupActivity implementation="com.github.bgomar.consolelogger.UpdateNotify" />
    <notificationGroup id="com.github.bgomar.consolelogger" displayType="STICKY_BALLOON" />
    <applicationService serviceImplementation="com.github.bgomar.consolelogger.ApplicationServicePlaceholder" id="com.github.bgomar.consolelogger" />
  </extensions>
  <actions>
    <group id="com.github.bgomar.consolelogger.ConsoleLoggerActionsGroup" class="com.github.bgomar.consolelogger.ConsoleLoggerActionsGroup" text="Log It" description="Generate javascript logs" popup="true">
      <add-to-group group-id="CodeMenu" anchor="last" />
      <action id="com.github.bgomar.consolelogger.addLog" class="com.github.bgomar.consolelogger.ConsoleLoggerAddLog" text="Insert ConsoleLogger's Console.Log" description="Generate a console.log() for that variable">
        <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt 1" />
        <keyboard-shortcut keymap="Mac OS X" first-keystroke="ctrl alt 1" />
      </action>
      <action id="com.github.bgomar.consolelogger.addInfo" class="com.github.bgomar.consolelogger.ConsoleLoggerAddInfo" text="Insert ConsoleLogger's Console.Info" description="Generate a console.info() for that variable">
        <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt 2" />
        <keyboard-shortcut keymap="Mac OS X" first-keystroke="ctrl alt 2" />
      </action>
      <action id="com.github.bgomar.consolelogger.addDebug" class="com.github.bgomar.consolelogger.ConsoleLoggerAddDebug" text="Insert ConsoleLogger's Console.Debug" description="Generate a console.debug() for that variable">
        <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt 3" />
        <keyboard-shortcut keymap="Mac OS X" first-keystroke="ctrl alt 3" />
      </action>
      <action id="com.github.bgomar.consolelogger.addWarn" class="com.github.bgomar.consolelogger.ConsoleLoggerAddWarn" text="Insert ConsoleLogger's Console.Warn" description="Generate a console.warn() for that variable">
        <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt 4" />
        <keyboard-shortcut keymap="Mac OS X" first-keystroke="ctrl alt 4" />
      </action>
      <action id="com.github.bgomar.consolelogger.addError" class="com.github.bgomar.consolelogger.ConsoleLoggerAddError" text="Insert ConsoleLogger's Console.Error" description="Generate a console.error() for that variable">
        <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt 5" />
        <keyboard-shortcut keymap="Mac OS X" first-keystroke="ctrl alt 5" />
      </action>
      <action id="com.github.bgomar.consolelogger.addTable" class="com.github.bgomar.consolelogger.ConsoleLoggerAddTable" text="Insert ConsoleLogger's Console.Table" description="Generate a console.table() for that variable">
        <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt 6" />
        <keyboard-shortcut keymap="Mac OS X" first-keystroke="ctrl alt 6" />
      </action>
      <action id="com.github.bgomar.consolelogger.removeLogs" class="com.github.bgomar.consolelogger.ConsoleLoggerRemove" text="Remove ConsoleLogger's Console.Log" description="Remove console.log() generate by ConsoleLogger plugin">
        <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt 0" />
        <keyboard-shortcut keymap="Mac OS X" first-keystroke="ctrl alt 0" />
      </action>
    </group>
  </actions>
</idea-plugin>
